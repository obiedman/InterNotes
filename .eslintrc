{
  "ecmaFeatures": {
    "modules": true
  },
   "env": {
    "es6": true,
    "browser": true,
    "jquery": true,
    "jasmine": true
  },
  "globals": {
    "chrome":true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": ["error", { allow: ["warn", "error", "log"] }]
  },
    strict: "global"
}
